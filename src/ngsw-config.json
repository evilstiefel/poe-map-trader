{
  "index": "/index.html",
  "dataGroups": [{
    "name": "api-cache",
    "urls": ["https://www.pathofexile.com/api/**"],
    "cacheConfig": {
      "strategy": "performance",
      "maxSize": 100,
      "maxAge": "1m"
    }
  }],
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js"
        ]
      }
    }, {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      }
    }
  ]
}
